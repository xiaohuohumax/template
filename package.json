{
  "name": "ts-vue-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "ts vue template",
  "author": {
    "name": "xiaohuohumax",
    "url": "https://github.com/xiaohuohumax"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaohuohumax/template.git"
  },
  "keywords": [
    "typescript",
    "vue",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "commit": "git add . && cz",
    "postinstall": "simple-git-hooks",
    "changeset": "changeset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@unocss/reset": "^0.65.1",
    "axios": "^1.7.9",
    "loglevel": "^1.9.2",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/mockjs": "^1.0.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "^9.17.0",
    "mockjs": "^1.1.0",
    "simple-git-hooks": "^2.11.1",
    "unocss": "^0.65.1",
    "vite": "^6.0.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-simple-html": "^0.2.0",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npm run lint:fix && git add ."
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
