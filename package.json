{
  "name": "ts-vue-template",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "ts vue template",
  "author": {
    "name": "xiaohuohumax",
    "url": "https://github.com/xiaohuohumax"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaohuohumax/template.git"
  },
  "keywords": [
    "typescript",
    "vue",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "commit": "git add . && cz",
    "postinstall": "simple-git-hooks",
    "changeset": "changeset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@unocss/reset": "^0.63.6",
    "axios": "^1.7.7",
    "loglevel": "^1.9.2",
    "pinia": "^2.2.5",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/mockjs": "^1.0.10",
    "@vitejs/plugin-vue": "^5.1.4",
    "commitizen": "^4.3.1",
    "cz-git": "^1.10.1",
    "eslint": "^9.13.0",
    "mockjs": "^1.1.0",
    "simple-git-hooks": "^2.11.1",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-simple-html": "^0.1.2",
    "vue-tsc": "^2.1.8"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npm run lint:fix && git add ."
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
